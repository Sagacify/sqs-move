{
  "name": "@sagacify/sqs-move",
  "version": "3.0.2",
  "description": "Moving SQS messages with ease",
  "main": "build/index.js",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "lint": "eslint --fix './src'",
    "test:lint": "eslint -f stylish 'src/**/*.ts'",
    "test:types": "tsc --noEmit",
    "test:spec": "env NODE_ENV=test mocha .",
    "test:cover": "nyc npm run test:spec",
    "test:watch": "env NODE_ENV=test mocha --watch",
    "test": "npm run test:lint && npm run test:types && npm run test:cover"
  },
  "author": "@oliviercuyp",
  "contributors": [
    "Fran√ßois Beuvens <francois@sagacify.com>",
    "Nicolas Vrielynck <nicolas@sagacify.com>",
    "Quentin Gusbin <quentin@sagacify.com>"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=12.4.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Sagacify/sqs-move"
  },
  "bugs": {
    "url": "https://github.com/Sagacify/sqs-move/issues"
  },
  "homepage": "https://github.com/Sagacify/sqs-move#readme",
  "keywords": [
    "AWS",
    "SQS",
    "queues",
    "move",
    "moving",
    "messages"
  ],
  "dependencies": {
    "@aws-sdk/client-sqs": "^3.55.0",
    "@types/lodash": "^4.14.173",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@types/chai": "^4.2.22",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.9.6",
    "@types/sinon": "^10.0.11",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "aws-sdk-client-mock": "^0.6.2",
    "chai": "^4.3.4",
    "coveralls": "^3.1.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-semistandard": "^16.0.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.1.0",
    "mocha": "^9.1.1",
    "nyc": "^15.1.0",
    "prettier": "^2.4.1",
    "sinon": "^11.1.2",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "plugins": [
      "@typescript-eslint",
      "prettier"
    ],
    "extends": [
      "semistandard",
      "plugin:@typescript-eslint/eslint-recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:prettier/recommended"
    ],
    "env": {
      "node": true,
      "mocha": true
    }
  },
  "mocha": {
    "require": "ts-node/register",
    "spec": [
      "test/**/*.ts"
    ],
    "watch-files": [
      "src/**/*.ts",
      "test/**/*.ts"
    ]
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "none",
    "bracketSpacing": true,
    "semi": true,
    "arrowParens": "always"
  },
  "release": {
    "branches": [
      "master"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git",
      "@semantic-release/github"
    ]
  }
}
